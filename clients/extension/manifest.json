{
  "name": "__MSG_appName__",
  "version": "5.1.0",
  "minimum_chrome_version": "72",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/lh_logo_bg.png",
    "128": "images/lh_logo_bg.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/lighthouse-background-page.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "38": "images/lh_logo_icon.png"
    },
    "default_title": "Lighthouse",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://googlechrome.github.io/*",
        "http://localhost:8000/*"
      ],
      "js": [
        "scripts/lighthouse-content-script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'none'",
  "externally_connectable": {
    "matches": [
      "https://googlechrome.github.io/*",
      "http://localhost:8000/*"
    ]
  }
}
